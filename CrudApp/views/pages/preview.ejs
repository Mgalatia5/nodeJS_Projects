<%- include('header') %>

    <main class="page projects-page">
        <section class="portfolio-block projects-cards">
            <div class="container">
                <iframe class=" w-100" style="min-height: 600px;" src="<%- project.liveLink %>" frameborder="0"></iframe>
                <div class="heading">
                    <h2>Recent Work</h2>
                </div>
                <div class="row">
                    <% var idDelay=500; %>
                        <div class="col-md-6 col-lg-4" data-aos="zoom-in-left" data-aos-delay="<% idDelay+200 %>">
                            <div class="card border-0"><a href="#"><img class="card-img-top scale-on-hover"
                                        src="../assets/img/nature/image1.jpg" alt="Card Image"></a>
                                <div class="card-body" style="box-shadow: 0px 3px 2px 0px;">
                                    <h6><a href="#">
                                            <%= project.title %>
                                        </a></h6>
                                    <p class="text-muted card-text">
                                        <%= project.description %>.
                                    </p>
                                    <a class=" btn btn-primary" href="preview/<%= project._id %>">Live Preview</a>
                                </div>
                            </div>
                        </div>
                </div>
            </div>
        </section>
    </main>
    <%- include('footer') %>